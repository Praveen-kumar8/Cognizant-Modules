import java.util.*;
public class OddorEvenChecker {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        System.out.println("Enter a Integer: ");
        int p = scan.nextInt();
        if(p%2==0)
        System.out.println("Even Number");
        else
        System.out.println("Odd Number");
    }
}
